{
    "name": "kiti/smart",
    "type": "project",
    "require": {
        "altorouter/altorouter": "^2.0",
        "filp/whoops": "^2.15",
        "gabordemooij/redbean": "^5.7",
        "phpmailer/phpmailer": "^6.9",
        "respect/validation": "^2.2",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@composer dump-autoload -o"
        ],
        "post-install-cmd": [
            "@composer dump-autoload -o"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
